pipeline {
    agent any
    stages{
        stage('Checkout') {
         steps {
               checkout([$class: 'GitSCM', branches: [[name: 'master,devbranch']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/sasemb/azure-voting-app-redis.git']]])
                                 
            }
        }
        stage('setvariable')
        {
            script {
                def branch = env.BRANCH_NAME
                switch(branch){
                    case 'master':
                        url = 'https://master.org'
                    case 'dev':
                        url = 'https://dev.org'
                }
                
                
            }
        }
        stage('run my script')
        {
            sh '''
                echo 'the URL is ${url} '
            '''
        }
    }
    
}